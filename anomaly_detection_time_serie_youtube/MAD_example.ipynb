   ],
   "source": [
    "import numpy as np\n",
    "\n",
    "# 数据集\n",
    "data = np.array([10, 12, 23, 23, 16, 23, 21, 16])\n",
    "\n",
    "# 计算中位数\n",
    "median = np.median(data)\n",
    "\n",
    "# 计算绝对偏差\n",
    "abs_deviation = np.abs(data - median)\n",
    "\n",
    "# 计算MAD\n",
    "mad = np.median(abs_deviation)\n",
    "\n",
    "print(\"Median:\", median)\n",
    "print(\"MAD:\", mad)\n"
   ]
  }
 ],
